{
    "sonarlint.pathToCompileCommands": "${workspaceFolder}/build/compile_commands.json",
    "cmake.debugConfig": {
        "name": "CMake Debug Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/src/app/application",
        "args": [
            "--help"
        ],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
    }
}
